<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/workoutback"
    tools:context=".workoutHomepage">

    <androidx.gridlayout.widget.GridLayout
        android:id="@+id/gridLayout3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <LinearLayout
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_margin="12dp"
            android:orientation="vertical"
            app:layout_column="0"
            app:layout_row="0">

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/walkhome" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/walkbackground" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="TODO"
                app:srcCompat="@drawable/walkbackground" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_margin="12dp"
            android:orientation="vertical"
            app:layout_column="0"
            app:layout_row="1">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="TODO"
                app:srcCompat="@drawable/runhome" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/runhome" />

            <ImageView
                android:id="@+id/imageView15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/runhome" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_margin="12dp"
            android:orientation="vertical"
            app:layout_column="0"
            app:layout_row="2">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="TODO"
                app:srcCompat="@drawable/cyclehome" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/cyclehome" />

            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/cyclehome" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_margin="12dp"
            android:orientation="vertical"
            app:layout_column="0"
            app:layout_row="3">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="TODO"
                app:srcCompat="@drawable/swimhome" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/swimhome" />

            <ImageView
                android:id="@+id/imageView17"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/swimhome" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="252dp"
            android:layout_height="100dp"
            android:layout_margin="12dp"
            android:orientation="vertical"
            app:layout_column="1"
            app:layout_row="0">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="@string/workoutHomeWalk"
                android:textAlignment="center"
                android:textColor="@android:color/holo_orange_light"
                android:textSize="36sp"
                android:textStyle="bold"
                tools:text="@string/workoutHomeWalk" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="252dp"
            android:layout_height="100dp"
            android:layout_margin="12dp"
            android:orientation="vertical"
            app:layout_column="1"
            app:layout_row="1">

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="@string/workoutHomeRun"
                android:textAlignment="center"
                android:textColor="@android:color/holo_blue_bright"
                android:textSize="36sp"
                android:textStyle="bold"
                tools:text="@string/workoutHomeRun" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="252dp"
            android:layout_height="100dp"
            android:layout_margin="12dp"
            android:orientation="vertical"
            app:layout_column="1"
            app:layout_row="2">

            <TextView
                android:id="@+id/textView10"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="@string/workoutHomeCycle"
                android:textAlignment="center"
                android:textColor="@android:color/holo_orange_light"
                android:textSize="36sp"
                android:textStyle="bold"
                tools:text="@string/workoutHomeCycle" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="252dp"
            android:layout_height="100dp"
            android:layout_margin="12dp"
            android:orientation="vertical"
            app:layout_column="1"
            app:layout_row="3">

            <TextView
                android:id="@+id/textView11"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="@string/WorkoutHomeSwim"
                android:textAlignment="center"
                android:textColor="@android:color/holo_blue_bright"
                android:textSize="36sp"
                android:textStyle="bold"
                tools:text="@string/WorkoutHomeSwim" />
        </LinearLayout>
    </androidx.gridlayout.widget.GridLayout>

    <androidx.gridlayout.widget.GridLayout
        android:layout_width="379dp"
        android:layout_height="101dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/gridLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <TextView
            android:id="@+id/textView12"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@string/WorkoutHomeWorkout"
            android:textAlignment="center"
            android:textSize="36sp"
            android:textStyle="bold"
            app:layout_column="0"
            app:layout_row="1"
            tools:text="@string/WorkoutHomeWorkout" />
    </androidx.gridlayout.widget.GridLayout>
</androidx.constraintlayout.widget.ConstraintLayout>